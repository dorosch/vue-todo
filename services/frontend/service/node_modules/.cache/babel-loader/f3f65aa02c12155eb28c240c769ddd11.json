)\n    },\n\n    addedItemForList: (context, payload) => {\n      API.post('/todo/items/', {\n        list: payload.id, text: payload.text\n      }).then(response => {\n        context.commit('addItemInListById', {\n          item: response.data\n        })\n      }).catch(error => {\n        console.log(error)\n      })\n    },\n\n    deleteItemById: (context, payload) => {\n      API.delete(`/todo/items/${payload.id}/`).then(function() {\n        context.commit('deleteItem', payload.id)\n      })\n      .catch(error => {\n        console.log(error)\n      })\n    }\n  }\n}\n\nexport default TodoModule\n"],"mappings":";;;;AAAA,OAAOA,GAAG,MAAM,gBAAgB;AAGhC,IAAMC,UAAU,GAAG;EACjBC,KAAK,EAAE;IACLC,KAAK,EAAE;EACT,CAAC;EAGDC,OAAO,EAAE;IACPC,QAAQ,EAAE,SAAAA,SAAAH,KAAK,EAAI;MACjB,OAAOA,KAAK,CAACC,KAAK;IACpB,CAAC;IAEDG,WAAW,EAAE,SAAAA,YAAAJ,KAAK;MAAA,OAAI,UAAAK,EAAE,EAAI;QAC1B,OAAOL,KAAK,CAACC,KAAK,CAACK,IAAI,CACrB,UAAAC,IAAI;UAAA,OAAIA,IAAI,CAACF,EAAE,KAAKA,EAAE;QAAA,CACxB,CAAC;MACH,CAAC;IAAA;EACH,CAAC;EAGDG,SAAS,EAAE;IACTC,OAAO,EAAE,SAAAA,QAACT,KAAK,EAAEO,IAAI,EAAK;MACxBP,KAAK,CAACC,KAAK,CAACS,IAAI,CAACH,IAAI,CAAC;IACxB,CAAC;IAEDI,iBAAiB,EAAE,SAAAA,kBAACX,KAAK,EAAEY,OAAO,EAAK;MACrCZ,KAAK,CAACC,KAAK,CAACK,IAAI,CACd,UAAAC,IAAI;QAAA,OAAIA,IAAI,CAACF,EAAE,KAAKO,OAAO,CAACC,IAAI,CAACN,IAAI;MAAA,CACvC,CAAC,CAACO,KAAK,CAACJ,IAAI,CAACE,OAAO,CAACC,IAAI,CAAC;IAC5B,CAAC;IAEDE,UAAU,EAAE,SAAAA,WAACf,KAAK,EAAEK,EAAE,EAAK;MACzBL,KAAK,CAACC,KAAK,CAACe,OAAO,CAAC,UAAAC,OAAO,EAAI;QAC7BA,OAAO,CAACH,KAAK,GAAGG,OAAO,CAACH,KAAK,CAACI,MAAM,CAClC,UAAAL,IAAI;UAAA,OAAIA,IAAI,CAACR,EAAE,KAAKA,EAAE;QAAA,CACxB,CAAC;MACH,CAAC,CAAC;IACJ;EACF,CAAC;EAGDc,OAAO,EAAE;IACPC,UAAU,EAAE,SAAAA,WAACC,OAAO,EAAK;MACvBvB,GAAG,CAACwB,GAAG,CAAC,cAAc,CAAC,CAACC,IAAI,CAAC,UAAAC,QAAQ,EAAI;QACvCA,QAAQ,CAACC,IAAI,CAACT,OAAO,CACnB,UAAAC,OAAO;UAAA,OAAII,OAAO,CAACK,MAAM,CAAC,SAAS,EAAET,OAAO,CAAC;QAAA,CAC/C,CAAC;MACH,CAAC,CAAC,CACDU,KAAK,CAAC,UAAAC,KAAK,EAAI;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ,CAAC;IAEDG,g